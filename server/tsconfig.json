{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "baseUrl": ".",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "dist",
        "paths": {
            "*": [
                "node_modules/*"
            ],
            "@config": ["config/config.json"],
            "@models": ["models/index.ts"],
            "@typings": ["typings/index.ts"]
        },
        "sourceMap": true,
        "resolveJsonModule": true,
        "target": "es6",
    },
}
