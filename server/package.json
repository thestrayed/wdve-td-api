{
    "name": "wdve-td-api",
    "version": "1.0.0",
    "main": "dist/server.js",
    "license": "MIT",
    "engines": {
        "node": "12.6.0"
      },
    "scripts": {
        "db:init": "yarn db:migrate && yarn db:seed",
        "db:migrate": "sequelize db:migrate",
        "db:seed": "sequelize db:seed:all",
        "dev": "ts-node-dev --respawn --transpileOnly server.ts",
        "lint": "tslint --project tsconfig.json",
        "test": "yarn test:unit && yarn test:integration",
        "test:integration": "jest --verbose --coverage --detectOpenHandles -c tests/integration/jest.config.integration.js ./integration",
        "test:unit": "jest --verbose --coverage -c tests/unit/jest.config.unit.js ./unit",
        "ci:test": "yarn ci:test:integration && yarn ci:test:unit",
        "ci:test:integration": "jest --verbose --coverage --detectOpenHandles --coverageReporters=text-lcov -c tests/integration/jest.config.integration.js ./integration | coveralls",
        "ci:test:unit": "jest --verbose --coverage --coverageReporters=text-lcov -c tests/unit/jest.config.unit.js ./unit | coveralls"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn lint"
        }
    },
    "dependencies": {
        "@hapi/joi": "15.0.0",
        "config": "3.0.1",
        "express": "4.16.4",
        "http-status": "1.3.2",
        "js-yaml": "3.12.2",
        "lodash": "4.17.13",
        "module-alias": "2.2.0",
        "pg": "7.10.0",
        "reflect-metadata": "0.1.13",
        "sequelize": "5.7.0"
    },
    "devDependencies": {
        "@types/bluebird": "3.5.26",
        "@types/config": "0.0.34",
        "@types/express": "4.16.1",
        "@types/faker": "4.1.5",
        "@types/hapi__joi": "15.0.1",
        "@types/http-status": "0.2.30",
        "@types/jest": "24.0.11",
        "@types/lodash": "4.14.123",
        "@types/node": "11.13.5",
        "@types/supertest": "2.0.7",
        "@types/umzug": "2.2.1",
        "@types/validator": "10.11.0",
        "coveralls": "3.0.3",
        "faker": "4.1.0",
        "husky": "2.3.0",
        "jest": "24.7.1",
        "node-mocks-http": "1.7.3",
        "sequelize-cli": "5.5.0",
        "supertest": "4.0.2",
        "ts-jest": "24.0.2",
        "ts-node": "8.0.3",
        "ts-node-dev": "1.0.0-pre.32",
        "tslint": "5.13.1",
        "typescript": "3.3.3333",
        "umzug": "2.2.0"
    },
    "_moduleAliases": {
        "@controllers/*": "controllers/*.js",
        "@config": "config/config.json",
        "@errors": "errors/index.js",
        "@factory": "tests/factory/index.js",
        "@middlewares": "middlewares/index.js",
        "@models": "model/index.js",
        "@routes": "routes/index.js",
        "@services": "services/index.js",
        "@tests/utilities/*": "tests/utilities/*.js",
        "@typings/*": "typings/*.js"
    }
}
